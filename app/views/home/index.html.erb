<!-- Intro Header -->
<section class="intro">
  <div class="contain container" >
    <div class="intro-body row">
      <div class="text_home col-lg-12 col-md-12 col-sm-12 col-xs-12" >
        <h1 class="brand-heading">Jeu de l'Euro 2016</h1>
        <p class="intro-text">Bienvenue sur notre site de paris en ligne</p>

        <a href="/lots" id="butPrizes" class="btn btn-default btn-lg">
          <span class="network-name">Découvrir tous les lots</span>
        </a>

        <i class="fa fa-angle-double-down animated"></i>
      </div>
    </div>
  </div>
</section>


<section id="nextMatch" class="form_match">
  <div class="filterMatchSect contain">
    <div class="container text-center lastBets">

      <% if @matches.nil? %>
          ERROR
      <% else %>
          <% @matches.each do |match| %>
              <div class="lastBet">

                <div class="betDesc">
                  <div class="col-lg-6">
                    <p>Date : <%= match.begin_at.httpdate %></p>
                  </div>
                  <div class="col-lg-6">
                    <p>Lieu : <%= match.stade.name %></p>
                  </div>
                </div>

                <div class="encartLastBet">
                  <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3 inheritFlag left">
                    <%= image_tag("public/england.jpg", :class => "flags")%>
                  </div>
                  <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6 inheritFlag">
                    <a href="" class="btn btn-circle_match ">
                      <i class="fa fa-github fa-fw"></i>
                      <span><b>Parier</b></span>
                    </a>
                  </div>
                  <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3 inheritFlag right">
                    <%= image_tag("public/sweden.png", :class => "flags") %>
                  </div>
                </div>
              </div>
          <% end %>
      <% end %>

    </div>
  </div>
</section>



<!-- Resultat Section -->

<section class="result">
  <div class="filtre_result">
    <section id="resultats" class="content-section text-center">
      <h2>Résultats</h2>
      <div class="col-lg-8 col-lg-offset-2">
        <table class="table panel-table" style="overflow-y: hidden; text-align: center">
          <thead>
          <tr>
            <th style="text-align: center"><b>Date</b></th>
            <th style="text-align: center"><b>Phase</b></th>
            <th style="text-align: center"><b>Match</b></th>
            <th style="text-align: center"><b>Score</b></th>
          </tr>
          </thead>
          <tbody>
          <tr>
            <td>10/06/16</td>
            <td>1/8</td>
            <td>France - Ukraine</td>
            <td>3-2</td>
          </tr>
          <tr>
            <td>10/06/16</td>
            <td>1/8</td>
            <td>France - Ukraine</td>
            <td>3-2</td>
          </tr>
          </tbody>
        </table>
        <a href="/results" class="btn btn-default btn-lg"><i class="fa fa-github fa-fw"></i> <span class="network-name">Voir plus</span></a>
        <!-- 51 matchs -->

      </div>
    </section>
  </div>
</section>

